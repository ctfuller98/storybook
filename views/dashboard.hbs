<h6>Dashboard</h6>
<h3>Weclome {{name}}</h3>
<p>Here is your bucket list! </p>
{{#if items}}
<table class="striped">
    <thead>
        <tr>
            <th>Title</th>
            <th>Type</th>
            <th>Status</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
       {{#each stories}} 
       <tr>
           <td><a href="/items/{{_id}}"></a>{{title}}</td>
           <td>{{createdAt}}</td>
           <td><span class="dash-status">{{status}}</span></td>
           <td></td>
       </tr>
       {{/each}}
    </tbody>
</table>

{{else}}
<p>You have not created any items</p>

{{/if}}